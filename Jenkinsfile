pipeline {
	agent any
		stages{
			stage('1-make a right'){
				steps{
					sh 'echo "walk..."'
				}
			}
			stage('2-make a left'){
				steps{
					sh 'echo "walk..."'
				}
			}
			stage('3-make a right'){
				steps{
					sh 'echo "walk.."'
				}
			}
			stage('4-cross the road'){
				steps{
					sh 'echo "Jump..."'
				}
			}
		}
}
